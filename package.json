{
  "name": "i18next-ssg",
  "version": "1.0.0",
  "description": "i18n ssg",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/commonjs/index.js"
    },
    "./serverSideTranslations": {
      "types": "./dist/types/serverSideTranslations.d.ts",
      "import": "./dist/esm/serverSideTranslations.js",
      "require": "./dist/commonjs/serverSideTranslations.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:es": "tsc --module es6 --moduleResolution node --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/commonjs",
    "build": "yarn clean & yarn build:es & yarn build:cjs",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "yrobot",
    "i18n",
    "ssg",
    "next.js"
  ],
  "author": "yrobot",
  "license": "ISC",
  "dependencies": {
    "next": "^12.3.1",
    "next-i18next": "^12.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^18.7.17",
    "@types/react": "^18.0.21",
    "typescript": "^4.8.3"
  }
}
