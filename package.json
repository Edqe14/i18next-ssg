{
  "name": "i18next-ssg",
  "version": "1.0.0",
  "description": "i18n ssg",
  "main": "dist/commonjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/types.d.ts",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build:es": "tsc ./src/index.ts --skipLibCheck --esModuleInterop --module es6 --moduleResolution node --outDir dist/es",
    "build:cjs": "tsc ./src/index.ts --skipLibCheck --esModuleInterop --module commonjs --outDir dist/commonjs",
    "build:types": "tsc --noEmit false --declaration --emitDeclarationOnly --outDir dist/types",
    "build": "npm run clean && npm run build:cjs && npm run build:es && npm run build:types",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "yrobot",
    "i18n",
    "ssg",
    "next.js"
  ],
  "author": "yrobot",
  "license": "ISC",
  "dependencies": {
    "next": "^12.3.1",
    "next-i18next": "^12.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.17",
    "@types/react": "^18.0.21",
    "typescript": "^4.8.3"
  }
}
